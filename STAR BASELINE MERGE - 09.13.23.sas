%macro seq(longdata,id,suffix);

proc sort data=&longdata;
by &id;
run;

data &longdata;
set &longdata;
by &id;
if first.&id then &suffix=1;
if &id = lag(&id) then &suffix+1;
run;

proc sort data=&longdata;
by &id &suffix;
run;

%mend;

