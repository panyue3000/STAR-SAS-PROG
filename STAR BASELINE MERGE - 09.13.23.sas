%covmerge (stblcovid=HNTOSSCV_STBLCOVID, stcovid=HNTOSSCF_STCOVID,new_var=NUMB_SIDE);
/*Tremors or involuntary muscle movements....................................................................*/  
%covmerge (stblcovid=HTIVMSCV_STBLCOVID, stcovid=HTIVMSCF_STCOVID,new_var=TREMORS);
/*Loss of balance, difficulty walking, or falls..............................................................*/  
%covmerge (stblcovid=HBLNCSCV_STBLCOVID, stcovid=HBLNCSCF_STCOVID,new_var=BALANCE);
/*Dizziness, sensation that the room is spinning, or feeling light-headed when you stand...............*/  
%covmerge (stblcovid=HDZYSCV_STBLCOVID, stcovid=HDZYSCF_STCOVID,new_var=DIZZINESS);
/*Problems with your memory..................................................................................*/  
%covmerge (stblcovid=HMEMSCV_STBLCOVID, stcovid=HMEMSCF_STCOVID,new_var=MEMORY);
/*Problems concentrating on things or feeling that you have "brain fog"......................................*/  
%covmerge (stblcovid=HBFOGSCV_STBLCOVID, stcovid=HBFOGSCF_STCOVID,new_var=CONCENTRATION);
/*Changes to your appetite...................................................................................*/  
%covmerge (stblcovid=HAPPTSCV_STBLCOVID, stcovid=HAPPTSCF_STCOVID,new_var=APPETITE);
/*Changes to your mood, for example feeling more anxious or depressed..................................*/  
%covmerge (stblcovid=HMOODSCV_STBLCOVID, stcovid=HMOODSCF_STCOVID,new_var=MOOD);
/*Insomnia (e.g., difficulty falling asleep or staying asleep)...............................................*/  
%covmerge (stblcovid=HINSMSCV_STBLCOVID, stcovid=HINSMSCF_STCOVID,new_var=INSOMNIA);
/*Hair loss..................................................................................................*/  
%covmerge (stblcovid=HHAIRSCV_STBLCOVID, stcovid=HHAIRSCF_STCOVID,new_var=HAIR_LOSS);
/*Seizures...................................................................................................*/  
%covmerge (stblcovid=HSEIZSCV_STBLCOVID, stcovid=HSEIZSCF_STCOVID,new_var=SEIZURES);
